#include<stdio.h>
#include<stdlib.h>
int main()
{
   int a,b,n,i,auxiliar;
   a=0;
   b=1;
   auxiliar=0;
   printf("entre com o numero inteiro:");
   scanf("%d",&n);
   printf("\n %d",a);
   printf("\n %d",b);
   
   for(i=0;i<n;i++)
   {
   	a=auxiliar;
   	auxiliar=a+b;
   	b=a;
   	printf("\n %d",auxiliar);
   }
   system("pause");
   return 0;
}
